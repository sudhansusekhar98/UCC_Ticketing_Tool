{
  "version": 2,
  "name": "ucc-ticketing-backend",
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}